{
  "name": "kubeops",
  "version": "0.1.0",
  "private": true,
  "main": "electron/main.js",
  "scripts": {
    "dev": "tsx server.ts",
    "build": "next build",
    "start": "NODE_ENV=production tsx server.ts",
    "lint": "eslint",
    "electron:dev": "concurrently -k \"npm run dev\" \"wait-on http://localhost:51230 && electron .\"",
    "electron:build": "npm run build && node scripts/build-server.mjs && electron-builder",
    "electron:build:mac": "npm run build && node scripts/build-server.mjs && electron-builder --mac",
    "electron:build:linux": "npm run build && node scripts/build-server.mjs && electron-builder --linux",
    "electron:build:win": "npm run build && node scripts/build-server.mjs && electron-builder --win"
  },
  "dependencies": {
    "@kubernetes/client-node": "^1.4.0",
    "@tanstack/react-table": "^8.21.3",
    "@types/dagre": "^0.7.53",
    "@xterm/addon-fit": "^0.11.0",
    "@xterm/addon-web-links": "^0.12.0",
    "@xterm/xterm": "^6.0.0",
    "@xyflow/react": "^12.10.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "dagre": "^0.8.5",
    "js-yaml": "^4.1.1",
    "lucide-react": "^0.563.0",
    "next": "16.1.6",
    "next-themes": "^0.4.6",
    "node-pty": "^1.1.0",
    "radix-ui": "^1.4.3",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "recharts": "^3.7.0",
    "sonner": "^2.0.7",
    "swr": "^2.4.0",
    "tailwind-merge": "^3.4.0",
    "ws": "^8.19.0",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/ws": "^8.18.1",
    "concurrently": "^9.0.0",
    "electron": "^33.0.0",
    "electron-builder": "^26.0.0",
    "esbuild": "^0.24.0",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "png-to-ico": "^3.0.1",
    "png2icons": "^2.0.1",
    "shadcn": "^3.8.4",
    "sharp": "^0.34.5",
    "tailwindcss": "^4",
    "tsx": "^4.21.0",
    "tw-animate-css": "^1.4.0",
    "typescript": "5.9.3",
    "wait-on": "^8.0.0"
  }
